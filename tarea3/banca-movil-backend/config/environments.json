{
  "dev": {
    "core_banking_endpoint": "https://dev-core-banking.internal.bank.com",
    "database_config": {
      "read_capacity_units": 5,
      "write_capacity_units": 5,
      "billing_mode": "PAY_PER_REQUEST"
    },
    "lambda_config": {
      "memory_size": 128,
      "timeout": 30,
      "reserved_concurrency": 10
    },
    "monitoring": {
      "log_level": "DEBUG",
      "enable_xray": true,
      "enable_detailed_monitoring": true
    },
    "security": {
      "enable_waf": false,
      "cors_origins": ["http://localhost:3000", "https://dev-app.bank.com"],
      "api_throttling": {
        "burst_limit": 100,
        "rate_limit": 50
      }
    }
  },
  "staging": {
    "core_banking_endpoint": "https://staging-core-banking.internal.bank.com",
    "database_config": {
      "read_capacity_units": 20,
      "write_capacity_units": 20,
      "billing_mode": "PAY_PER_REQUEST"
    },
    "lambda_config": {
      "memory_size": 256,
      "timeout": 30,
      "reserved_concurrency": 50
    },
    "monitoring": {
      "log_level": "INFO",
      "enable_xray": true,
      "enable_detailed_monitoring": true
    },
    "security": {
      "enable_waf": true,
      "cors_origins": ["https://staging-app.bank.com"],
      "api_throttling": {
        "burst_limit": 500,
        "rate_limit": 250
      }
    }
  },
  "prod": {
    "core_banking_endpoint": "https://core-banking.internal.bank.com",
    "database_config": {
      "read_capacity_units": 100,
      "write_capacity_units": 100,
      "billing_mode": "PAY_PER_REQUEST"
    },
    "lambda_config": {
      "memory_size": 512,
      "timeout": 30,
      "reserved_concurrency": 200
    },
    "monitoring": {
      "log_level": "WARN",
      "enable_xray": true,
      "enable_detailed_monitoring": true
    },
    "security": {
      "enable_waf": true,
      "cors_origins": ["https://app.bank.com"],
      "api_throttling": {
        "burst_limit": 2000,
        "rate_limit": 1000
      }
    },
    "backup": {
      "enable_point_in_time_recovery": true,
      "backup_retention_period": 30
    }
  }
}
